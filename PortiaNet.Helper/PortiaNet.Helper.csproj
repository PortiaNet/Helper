<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Title>PortiaNet .Net Helper</Title>
    <Authors>Amin Esmaeily</Authors>
    <Description>PortiaNet.Helper contains some helpful methods and functions to speedup the work and reduce duplicate works.</Description>
    <PackageProjectUrl>https://github.com/PortiaNet/Helper</PackageProjectUrl>
    <PackageIcon>settings.png</PackageIcon>
    <RepositoryUrl>https://github.com/PortiaNet/Helper</RepositoryUrl>
    <PackageReadmeFile>readme.md</PackageReadmeFile>
    <PackageTags>.Net; Helper; Tools; Class Library; .Net 6; .Net 7</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Pagination.cs" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\readme.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
    <None Include="Pagination.cs" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="..\..\..\..\..\settings.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore" Version="7.0.0" />
  </ItemGroup>

</Project>
